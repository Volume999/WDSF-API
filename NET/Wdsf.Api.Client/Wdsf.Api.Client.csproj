<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Wdsf.Api.Client</AssemblyName>
    <RootNamespace>Wdsf.Api.Client</RootNamespace>
    <Version>2.0.0</Version>
    <Authors>John Caprez</Authors>
    <Company>JayKay-Design S.C.</Company>
    <PackageLicenseExpression>LGPL</PackageLicenseExpression>
    <Description>A .NET library which connects to the World DanceSport Federation's API using strongly typed models.</Description>
    <PackageReleaseNotes>Target .netstandard 2</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/jaykay-design/WDSF-API/</RepositoryUrl>
    <PackageTags>wdsf,api, dancesport</PackageTags>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Wdsf.Api.Client.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
  </ItemGroup>

</Project>
